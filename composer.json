{
    "name": "talentui33/active-campaign",
    "description": "Integration and API Consuming for ActiveCampaign",
    "keywords": [
        "laravel",
        "activecampaign",
        "api"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Talentui33",
            "email": "oscar@talentu.co"
        },
        {
            "name": "Kevin Cifuentes",
            "email": "cifuenteskevin85@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "laravel/framework": ">=5.8",
        "guzzlehttp/guzzle": "^6.3",
        "ext-json": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^3.8"
    },
    "autoload": {
        "psr-4": {
            "TalentuI33\\ActiveCampaign\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "TalentuI33\\ActiveCampaign\\ActiveCampaignServiceProvider"
            ],
            "aliases": {
                "ActiveCampaign": "TalentuI33\\ActiveCampaign\\Facade"
            }
        }
    }
}
